{
    "compilerOptions": {
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "baseUrl": "./",
      "sourceMap": true,
      "module": "esnext",
      "target": "esnext",
      "jsx": "react",
      "allowJs": true,
      "noEmit": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "lib": ["es5", "dom"],
      "resolveJsonModule": true
    },
  }